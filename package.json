{
  "name": "daisyui-matsu-theme",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  },
  "packageManager": "bun@latest",
  "scripts": {
    "dev": "bun run --bun vite",
    "build": "bun run tsc && bun run vite build",
    "preview": "bun run vite preview",
    "lint": "bun run eslint .",
    "test": "bun run vite build && bun run vite preview --port 8080",
    "predeploy": "bun run build",
    "deploy": "gh-pages -d dist",
    "clean": "rm -rf dist node_modules/.vite"
  },
  "description": "A Studio Ghibli inspired theme for DaisyUI - bringing the magical aesthetics of Ghibli films to your web projects with beautiful, whimsical components.",
  "author": "M.Hossein",
  "license": "MIT",
  "homepage": "https://github.com/HMarzban/daisyui-matsu-theme",
  "repository": {
    "type": "git",
    "url": "https://github.com/HMarzban/daisyui-matsu-theme.git"
  },
  "dependencies": {
    "@fontsource/nunito": "^5.2.5",
    "@fontsource/pt-serif": "^5.2.5",
    "daisyui": "^5.0.16",
    "highlight.js": "11.11.1",
    "lucide-react": "^0.487.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "recharts": "^2.15.2",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.3",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/webfontloader": "^1.6.38",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "gh-pages": "^6.3.0",
    "postcss": "^8.5.3",
    "pwa-asset-generator": "^8.0.4",
    "tailwindcss": "^4.1.3",
    "typescript": "^5",
    "vite": "^6.2.5",
    "vite-plugin-pwa": "^1.0.0",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^5.1.4",
    "workbox-window": "^7.3.0"
  }
}
